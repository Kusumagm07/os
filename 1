#include <stdio.h>

int main() {
    int n, i;
    printf("Enter the number of elements: ");
    scanf("%d", &n);
    
    int arr[n];
    printf("Enter the elements: ");
    for (i = 0; i < n; i++) {
        scanf("%d", &arr[i]);
    }

    int first = arr[0], second = -1;
    for (i = 1; i < n; i++) {
        if (arr[i] < first) {
            second = first;
            first = arr[i];
        } else if (arr[i] < second || second == -1) {
            second = arr[i];
        }
    }

    if (second == -1) {
        printf("No second smallest element found.\n");
    } else {
        printf("The second smallest element is: %d\n", second);
    }

    return 0;
}
