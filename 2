#include <stdio.h>

int main() {
    int n, i, j, sum = 0;

    // Get the size of the matrix (n x n)
    printf("Enter the size of the matrix: ");
    scanf("%d", &n);

    int matrix[n][n];

    // Input the matrix elements
    printf("Enter the elements of the matrix:\n");
    for (i = 0; i < n; i++) {
        for (j = 0; j < n; j++) {
            scanf("%d", &matrix[i][j]);
        }
    }

    // Calculate the sum of the left diagonal
    for (i = 0; i < n; i++) {
        sum += matrix[i][i];  // The element at [i][i] is part of the left diagonal
    }

    // Output the sum of the left diagonal
    printf("The sum of the left diagonal is: %d\n", sum);

    return 0;
}
